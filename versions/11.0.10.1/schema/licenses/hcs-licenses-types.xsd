				<xs:element name="LicenseRegDate" type="xs:date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата регистрации лицензии</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:element name="LicenseStatus">
