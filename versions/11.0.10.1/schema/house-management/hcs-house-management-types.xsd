			<xs:element name="FactorySealDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата опломбирования ПУ заводом-изготовителем (обязательно для заполнения при импорте)</xs:documentation>
				</xs:annotation>
