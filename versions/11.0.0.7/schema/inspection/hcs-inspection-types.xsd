	<xs:import namespace="http://dom.gosuslugi.ru/schema/integration/premises-base/" schemaLocation="../lib/hcs-premises-base.xsd"/>
	<!-- Скалярные типы -->
	<xs:simpleType name="InspectionPlanVersionStateType">
		<xs:annotation>
			<xs:documentation>Статус версии плана проверок. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Draft">
				<xs:annotation>
					<xs:documentation>Черновик.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Signed">
				<xs:annotation>
					<xs:documentation>Подписан.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PlannedExaminationVersionStateType">
		<xs:annotation>
			<xs:documentation>Статус версии плановой проверки. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Draft">
				<xs:annotation>
					<xs:documentation>Черновик.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Active">
				<xs:annotation>
					<xs:documentation>Опубликована.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PlannedExaminationStateType">
		<xs:annotation>
			<xs:documentation>Статус плановой проверки.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Planned">
				<xs:annotation>
					<xs:documentation>Запланирована.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Cancelled">
				<xs:annotation>
					<xs:documentation>Отменена.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Finished"/>
			<xs:enumeration value="Annulled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExaminationVersionStateType">
		<xs:annotation>
			<xs:documentation>Статус версии проверки.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Draft">
				<xs:annotation>
					<xs:documentation>Черновик.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Active">
				<xs:annotation>
					<xs:documentation>Опубликована.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExaminationStateType">
		<xs:annotation>
			<xs:documentation>Статус проверки.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Planned">
				<xs:annotation>
					<xs:documentation>Выполняется.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Finished">
				<xs:annotation>
					<xs:documentation>Завершена.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Cancelled">
				<xs:annotation>
					<xs:documentation>Отменена.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Annulled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PreceptStateType">
		<xs:annotation>
			<xs:documentation>Статус предписания.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Active">
				<xs:annotation>
					<xs:documentation>Не отменено (не отозвано).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Cancelled">
				<xs:annotation>
					<xs:documentation>Отменено (отозвано).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Fulfiled"/>
			<xs:enumeration value="NotFilled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OffenceStateType">
		<xs:annotation>
			<xs:documentation>Статус протокола.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Active">
				<xs:annotation>
					<xs:documentation>Не отменен.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Cancelled">
				<xs:annotation>
					<xs:documentation>Отменен.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Fulfiled"/>
			<xs:enumeration value="NotFilled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String50Type">
		<xs:annotation>
			<xs:documentation>Строка не более 50 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String30Type">
		<xs:annotation>
			<xs:documentation>Строка не более 30 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String255Type">
		<xs:annotation>
			<xs:documentation>Строка не более 255 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String2000Type">
		<xs:annotation>
			<xs:documentation>Строка не более 2000 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TimeUnitCountType">
		<xs:annotation>
			<xs:documentation>Число временных единиц.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumberType">
		<xs:annotation>
			<xs:documentation>Порядковый номер.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FunctionRegistryNumberType">
		<xs:annotation>
			<xs:documentation>Реестровый номер функции органа жилищного надзора в системе «Федеральный реестр государственных и муниципальных услуг»*</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,36}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RegulatoryAuthorityIDType">
		<xs:annotation>
			<xs:documentation>Идентификатор контролирующего органа в системе «Федеральный реестр государственных и муниципальных услуг»*</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,36}"/>
		</xs:restriction>
	</xs:simpleType>
