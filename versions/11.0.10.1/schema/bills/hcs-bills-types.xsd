									<xs:sequence>
										<xs:element name="PeriodFrom" type="tns:ReportPeriodType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Период с (Если не заполнено, то без ограничения начального периода)</xs:documentation>
											</xs:annotation>
