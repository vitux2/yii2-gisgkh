						<xs:element name="Contract" type="tns:exportSupplyResourceContractResultType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Договор ресурсоснабжения</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:attribute ref="base:version" use="required" fixed="10.0.2.1"/>
				</xs:extension>
