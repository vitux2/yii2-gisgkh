	<xs:complexType name="InspectionPlanType">
		<xs:annotation>
			<xs:documentation>Основные реквизиты плана проверок.(экспорт)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Year" type="base:YearType">
				<xs:annotation>
					<xs:documentation>Год плана.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="ShouldNotBeRegistered" type="xs:boolean" fixed="true">
					<xs:annotation>
						<xs:documentation>Не должен быть зарегистрирован в Едином реестре проверок.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="URIRegistrationNumber">
					<xs:annotation>
						<xs:documentation>Регистрационный номер плана в едином реестре проверок</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:minInclusive value="0"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PlannedExaminationType">
		<xs:annotation>
			<xs:documentation>Плановая проверка (пункт плана проверок)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PlannedExaminationOverview">
				<xs:annotation>
					<xs:documentation>Общая информация о проверке</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="NumberInPlan">
							<xs:annotation>
								<xs:documentation>Номер проверки в плане</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tns:NumberType">
									<xs:totalDigits value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="URIRegistrationNumber" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Учетный номер проверки в едином реестре проверок</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="URIRegistrationDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата присвоения учетного номера проверки</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Subject" type="tns:ScheduledExaminationSubjectInPlanInfoType">
				<xs:annotation>
					<xs:documentation>Субъект проверки.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlannedExaminationInfo">
