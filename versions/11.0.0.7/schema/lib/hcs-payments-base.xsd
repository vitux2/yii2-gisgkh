																<xs:restriction base="xs:string">
																	<xs:minLength value="1"/>
																	<xs:maxLength value="255"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
														<xs:element name="Placement" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Номер комнаты жилого помещения</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:minLength value="1"/>
