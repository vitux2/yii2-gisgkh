	<xs:element name="importInspectionPlanRequest">
		<xs:annotation>
			<xs:documentation>Запрос на импорт плана проверок.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="base:BaseType">
					<xs:sequence>
						<xs:element name="ImportInspectionPlan" maxOccurs="1000">
							<xs:annotation>
								<xs:documentation>Создание/изменение плана проверок</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:sequence>
										<xs:element ref="base:TransportGUID"/>
										<xs:element name="InspectionPlanGuid" type="base:GUIDType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Идентификатор плана проверок в ГИС ЖКХ.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:choice>
										<xs:sequence>
											<xs:element name="Sign" type="xs:boolean" fixed="true" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Признак подписания плана проверок для публикации в ГИС ЖКХ. </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="InspectionPlan" type="tns:InspectionPlanType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Добавляемый/изменяемый план проверок. Не заполяется при выполнении операций только с плановыми проверками.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ImportPlannedExamination" minOccurs="0" maxOccurs="1000">
												<xs:annotation>
													<xs:documentation>Создание/изменение плановой проверки в плане проверок.</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:sequence>
															<xs:element ref="base:TransportGUID"/>
															<xs:element name="PlannedExaminationGuid" type="base:GUIDType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Идентификатор плановой проверки в ГИС ЖКХ.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
														<xs:element name="PlannedExamination" type="tns:PlannedExaminationType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Плановая проверка.</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="CancelPlannedExamination" type="tns:CancelledInfoWithAttachmentsType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Отмена плановой проверки.</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="DeletePlannedExamination" type="xs:boolean" fixed="true" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Удаление плановой проверки</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="AnnulPlannedExamination" type="xs:boolean" fixed="true" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Аннулирование плановой проверки</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:element name="DeleteInspectionPlan" type="xs:boolean" fixed="true" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Удаление плана проверок. Также удаляются все плановые проверки.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute ref="base:version" use="required" fixed="10.0.1.1"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="importInspectionPlanRequest_TransportGUIDKey">
			<xs:selector xpath=".//base:TransportGUID"/>
			<xs:field xpath="."/>
		</xs:key>
	</xs:element>
